            </actions>
            <offsetFromField>Opportunity.RFP_Due_Date__c</offsetFromField>
            <timeLength>10</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>Opportunity Audit request notification</fullName>
        <actions>
            <name>Opportunity_Audit_request_notification_ae</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <formula>ISPICKVAL(StageName, &apos;Evaluation&apos;) &amp;&amp; Audit__c &amp;&amp; !Email_Notifications_Off__c</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Opportunity Evaluation notification</fullName>
        <actions>
            <name>Opportunity_Evaluation_notification_ae</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>ISPICKVAL(StageName, &apos;Evaluation&apos;) &amp;&amp; 
ISCHANGED(StageName) &amp;&amp;
 RecordType.Name != &apos;SaaS&apos; &amp;&amp;
!ISNEW() &amp;&amp;
!Email_Notifications_Off__c</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Opportunity Evaluation notification New Opp</fullName>
        <active>true</active>
        <formula>ISPICKVAL(StageName, &apos;Evaluation&apos;) &amp;&amp; 
 RecordType.Name != &apos;SaaS&apos; &amp;&amp;
!Email_Notifications_Off__c</formula>
        <triggerType>onCreateOnly</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Opportunity_Evaluation_notification_ae</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Opportunity.LastModifiedPlus1min__c</offsetFromField>
            <timeLength>0</timeLength>
            <workflowTimeTriggerUnit>Hours</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>Opportunity Lost</fullName>
        <actions>
            <name>Opportunity_Lost</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>!ISNEW() &amp;&amp;  ISCHANGED(StageName) &amp;&amp; ISPICKVAL(StageName,&quot;Closed Lost&quot;) &amp;&amp; !Email_Notifications_Off__c</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Opportunity Pending Stage</fullName>
        <actions>
            <name>Opportunity_Pending_Stage</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
