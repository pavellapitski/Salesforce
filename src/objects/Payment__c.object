    <fields>
        <fullName>Strike_Final_Margin__c</fullName>
        <externalId>false</externalId>
        <formula>IF(BEGINS(Project__r.BillingCondition__r.Name,&quot;Rebate&quot;), Project__r.BillingCondition__r.Default_Margin__c - Project__r.BillingCondition__r.Rebate_Rate__c,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
