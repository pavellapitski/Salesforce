    <validationRules>
        <fullName>AgeMinMinimum13</fullName>
        <active>true</active>
        <errorConditionFormula>!ISBLANK(Age_Min__c) &amp;&amp;
OR(PlatformPrefix__c=&quot;FB&quot;,
PlatformPrefix__c=&quot;IG&quot;,
PlatformPrefix__c=&quot;SN&quot;) &amp;&amp;
Age_Min__c&lt;13</errorConditionFormula>
        <errorDisplayField>Age_Min__c</errorDisplayField>
        <errorMessage>Minimum Age is 13</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DefineCostStructure</fullName>
        <active>true</active>
        <errorConditionFormula>not(BypassValidationRule__c) &amp;&amp; 
!priorvalue(BypassValidationRule__c) &amp;&amp;
AND(ISBLANK(TEXT(CostStructure__c)),  NOT(OpportunityPlatform__r.NoCostStructure__c))</errorConditionFormula>
        <errorDisplayField>CostStructure__c</errorDisplayField>
        <errorMessage>Please define Cost Structure</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DefineUnitCost</fullName>
        <active>true</active>
        <errorConditionFormula>not(BypassValidationRule__c) &amp;&amp; 
!priorvalue(BypassValidationRule__c) &amp;&amp;
AND(ISNULL(UnitCost__c), NOT(OpportunityPlatform__r.NoCostStructure__c),
VariableUnitCost__c)</errorConditionFormula>
        <errorDisplayField>UnitCost__c</errorDisplayField>
        <errorMessage>Please define Unit Cost</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EndDateAboveMaxSpendDate</fullName>
        <active>true</active>
        <errorConditionFormula>MaxMediaSpendDate__c &gt; End_Date__c</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>You cannot adjust this date as Media Spend date for this Project is higher than the date you intend to use.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ProjectMarginNotEditable</fullName>
        <active>true</active>
        <errorConditionFormula>not(BypassValidationRule__c) &amp;&amp; 
!priorvalue(BypassValidationRule__c) &amp;&amp;
Margin__c != Project__r.Margin__c &amp;&amp; ischanged(Margin__c) &amp;&amp;
 $User.Id != &apos;005U0000003nBsV&apos;</errorConditionFormula>
        <errorDisplayField>Margin__c</errorDisplayField>
        <errorMessage>Please define margin on Contract level.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>StartDateAboveMinSpendDate</fullName>
        <active>true</active>
        <errorConditionFormula>MinMediaSpendDate__c &lt; Start_Date__c</errorConditionFormula>
        <errorDisplayField>Start_Date__c</errorDisplayField>
        <errorMessage>You cannot adjust this date as Media Spend date for this Project is lower than the date you intend to use. Please review the dates.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
