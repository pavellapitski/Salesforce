        <relationshipLabel>Projects</relationshipLabel>
        <relationshipName>Payments</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Quantity__c</fullName>
        <externalId>false</externalId>
        <formula>if(UnitCost__c&lt;&gt;0, if(ISPICKVAL(CostStructure__c, &apos;CPM&apos;), Client_Spend__c/UnitCost__c * 1000, Client_Spend__c/UnitCost__c), 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
