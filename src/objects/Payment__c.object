        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>SuggestedSpend__c</fullName>
        <externalId>false</externalId>
        <formula>IF (UnitCost__c != 0 &amp;&amp; AmountDelivered__c != 0,
  IF(
    (IF(TEXT(CostStructure__c) == &apos;CPM&apos;,
      AdWordsSpend__c / AmountDelivered__c * 1000,
      AdWordsSpend__c / AmountDelivered__c)
    ) * IF(ISPICKVAL(Margin_Type__c,&quot;% of Media&quot;), (1+Margin__c), 1/(1-Margin__c)) &gt;= UnitCost__c,
    IF(TEXT(CostStructure__c) == &apos;CPM&apos;,
      AmountDelivered__c * UnitCost__c / 1000,
      AmountDelivered__c * UnitCost__c
    ),
    AdWordsSpend__c * IF(ISPICKVAL(Margin_Type__c,&quot;% of Media&quot;), (1+Margin__c), 1/(1-Margin__c))
  ),
  AdWordsSpend__c * IF(ISPICKVAL(Margin_Type__c,&quot;% of Media&quot;), (1+Margin__c), 1/(1-Margin__c))
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Suggested Spend</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
