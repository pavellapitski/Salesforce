    <validationRules>
        <fullName>ContractOrOpportunityRequired</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISBLANK(Contract__c), ISBLANK(Opportunity__c))</errorConditionFormula>
        <errorMessage>Please define Contract or Opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FillUrlWhenNotPlaceholder</fullName>
        <active>true</active>
        <errorConditionFormula>!IsPlaceholder__c &amp;&amp; ISBLANK(Url__c) &amp;&amp; !ISPICKVAL(Format__c,&quot;Page Post&quot;)</errorConditionFormula>
        <errorDisplayField>Url__c</errorDisplayField>
        <errorMessage>Non-placeholder URL cannot be empty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FormatRequired</fullName>
        <active>true</active>
        <errorConditionFormula>ispickval(Format__c,&quot;&quot;)</errorConditionFormula>
        <errorDisplayField>Format__c</errorDisplayField>
        <errorMessage>Please select Format</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IncorrectYTFormat</fullName>
        <active>false</active>
        <errorConditionFormula>OR(
ISPICKVAL(Platform__c,&quot;TrueView&quot;),
ISPICKVAL(Platform__c,&quot;AdWords CPM&quot;)
)
&amp;&amp;
!ISPICKVAL(Format__c,&quot;Video&quot;)</errorConditionFormula>
        <errorMessage>Incorrect format of Creative</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>InvalidYouTubeUrl</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  NOT(ISBLANK(Url__c)), 
  ispickval(Format__c,&quot;Video&quot;),
  NOT(REGEX(Url__c, &apos;(^(http(s)?://)?)(www.)?((youtube.[a-z]+/)|(youtu.be/))[\\S]+&apos;)),
  ISPICKVAL(Format__c,&quot;Video&quot;),
  OR(ISPICKVAL(Platform__c,&quot;AdWords CPM&quot;),
     ISPICKVAL(Platform__c,&quot;TrueView&quot;))
)</errorConditionFormula>
        <errorDisplayField>Url__c</errorDisplayField>
        <errorMessage>Invalid format for YouTube URL</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NameCannotContainBlankspacesByHypehen</fullName>
        <active>true</active>
        <errorConditionFormula>CONTAINS(Name,&quot; - &quot;)</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Name cannot contain blank spaces around hyphens</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PlaceholderCannotHaveURL</fullName>
        <active>true</active>
        <errorConditionFormula>IsPlaceholder__c &amp;&amp; !ISBLANK(Url__c)</errorConditionFormula>
        <errorDisplayField>Url__c</errorDisplayField>
        <errorMessage>Placeholder cannot have URL</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rotation0to100</fullName>
        <active>true</active>
        <errorConditionFormula>Rotation__c &gt; 1 || Rotation__c &lt; -0.01</errorConditionFormula>
        <errorDisplayField>Rotation__c</errorDisplayField>
        <errorMessage>Rotation is value between -1% and 100% where -1% means &apos;No Rotation&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>StartDateNotBiggerThanEndDate</fullName>
        <active>true</active>
        <errorConditionFormula>StartDate__c &gt; EndDate__c</errorConditionFormula>
        <errorMessage>End Date cannot be before Start Date</errorMessage>
    </validationRules>
