    <validationRules>
        <fullName>RebateRateValidation</fullName>
        <active>false</active>
        <description>100% is a maximum value</description>
        <errorConditionFormula>IF(OR(Rebate_Rate__c &lt;= 100 &amp;&amp; RecordType.Name == &apos;Rebate&apos;, RecordType.Name == &apos;Normal&apos;), false, true)</errorConditionFormula>
        <errorDisplayField>Rebate_Rate__c</errorDisplayField>
        <errorMessage>The maximum value is 100</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecordTypeValidation</fullName>
        <active>false</active>
        <description>Allow to populate the value only when Record Type is &quot;Rebate&quot;</description>
        <errorConditionFormula>IF(OR(RecordType.DeveloperName = &apos;Rebate&apos;, RecordType.DeveloperName = &apos;Normal&apos; &amp;&amp; ISBLANK(Rebate_Rate__c )), false, true)</errorConditionFormula>
        <errorDisplayField>Rebate_Rate__c</errorDisplayField>
        <errorMessage>The value is allowed only for Rebate Billing Conditions</errorMessage>
    </validationRules>
