    <fields>
        <fullName>Xero_Invoice__c</fullName>
        <externalId>false</externalId>
        <formula>IF(Xero_ID__c==&apos;No Invoice&apos; || Invoice__c==&apos;No Invoice&apos;,
&apos;No Invoice&apos;,
IF(!ISBLANK(Xero_ID__c),
HYPERLINK(&quot;https://go.xero.com/AccountsReceivable/Edit.aspx?InvoiceID=&quot; &amp; Xero_ID__c,
IF(!ISBLANK(Invoice__c),Invoice__c,&quot;View&quot;)),
&quot;Not Available&quot;)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>If you encounter error after clicking on the &apos;View&apos; link please log into Xero and switch the instance.</inlineHelpText>
        <label>Xero Invoice</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
