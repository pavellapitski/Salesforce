    <fields>
        <fullName>Suggested_Client_Spend__c</fullName>
        <externalId>false</externalId>
        <formula>IF(AND(TODAY()&gt;=End_Date__c+1, Margin__c!=1),
   SuggestedSpend__c,
NULL)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
