        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Total_Revenue__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(Account_Type__c, &quot;Client&quot;), Client_Spend__c - Media_Order__c, Client_Spend__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Expected Revenue</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Total_Revenue_report__c</fullName>
        <externalId>false</externalId>
        <formula>if(Synced_with_Xero__c || !ispickval( XeroStatus__c,&quot;&quot;), Invoice_Amount__c,Total_Revenue__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Actual Revenue</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
