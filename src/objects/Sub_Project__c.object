        <fullName>CannotEditPaymentAfterInvoice</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(BypassValidationRule__c) &amp;&amp; 
!priorvalue(BypassValidationRule__c) &amp;&amp;
AND($Profile.Name != &apos;System Administrator&apos;, $User.Id !=&quot;005U0000003mAzn&quot;,$User.CanDeletePPs__c = FALSE, 
NOT(ISBLANK(Xero_ID__c)), 
OR(ISCHANGED(Client_Spend__c), ISCHANGED(Start_Date__c), ISCHANGED(End_Date__c)
)
)</errorConditionFormula>
        <errorMessage>Cannot update Payment after Invoice has been issued</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PaymentMarginNotEditable</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(BypassValidationRule__c) &amp;&amp; 
!priorvalue(BypassValidationRule__c) &amp;&amp;
Margin__c != Project__r.Margin__c &amp;&amp; ischanged(Margin__c)</errorConditionFormula>
        <errorDisplayField>Margin__c</errorDisplayField>
        <errorMessage>Please define margin on Contract level.</errorMessage>
    </validationRules>
