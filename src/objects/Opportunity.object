!PRIORVALUE(BypassValidationRule__c) &amp;&amp;
ISBLANK(BillingConditionName__c) &amp;&amp; 
ISPICKVAL(StageName,&apos;Closed Won&apos;) &amp;&amp;
RecordTypeId != $Label.Opportunity_Record_Type_SaaS</errorConditionFormula>
        <errorDisplayField>BillingCondition__c</errorDisplayField>
        <errorMessage>Billing Conditions value is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BrandRequiredFromNegotiatingStage</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(BypassValidationRule__c) &amp;&amp; 
!PRIORVALUE(BypassValidationRule__c) &amp;&amp; 
OR(ISPICKVAL(StageName,&apos;Negotiating &amp; Mutual Plan&apos;),
ISPICKVAL(StageName,&apos;Finalizing Closure&apos;),
ISPICKVAL(StageName,&apos;Pending&apos;),
ISPICKVAL(StageName,&apos;Closed Won&apos;)) &amp;&amp;
ISBLANK( Brand__c ) &amp;&amp;
RecordTypeId != $Label.Opportunity_Record_Type_SaaS</errorConditionFormula>
        <errorDisplayField>Brand__c</errorDisplayField>
        <errorMessage>Please fill in Brand value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BrandRequiredFromProposalStage</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(BypassValidationRule__c) &amp;&amp; 
!PRIORVALUE(BypassValidationRule__c) &amp;&amp; 
(ISPICKVAL(StageName,&apos;Closed Won&apos;) ||
ISPICKVAL(StageName,&apos;Proposal/Price Quote&apos;)) &amp;&amp;
ISBLANK( Brand__c ) &amp;&amp;
RecordTypeId != $Label.Opportunity_Record_Type_SaaS</errorConditionFormula>
        <errorDisplayField>Brand__c</errorDisplayField>
        <errorMessage>Please fill in Brand value</errorMessage>
    </validationRules>
