        <errorConditionFormula>NOT(BypassValidationRule__c) &amp;&amp;
!PRIORVALUE(BypassValidationRule__c) &amp;&amp;
ISPICKVAL( StageName , &quot;Evaluation&quot;) &amp;&amp;
(Audit__c || Proposal__c) &amp;&amp;
ISBLANK(RFP_Due_Date__c) &amp;&amp;
RecordTypeId != $Label.Opportunity_Record_Type_SaaS</errorConditionFormula>
        <errorDisplayField>RFP_Due_Date__c</errorDisplayField>
        <errorMessage>Please provide Audi/Proposal Due Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BillingConditionRequiredWhenClosedWon</fullName>
        <active>true</active>
